pkgname=picasa_upload
pkgver=0.1.5
pkgrel=2
pkgdesc="A command line utility to upload a bunch of photos to picasa."
arch=('any')
url=('http://www.fifthplanet.net')
license=('GPL3')
depends=('python2-gdata')
source=(picasa_upload)
md5sums=('933901c3e87acf0f7341948dfc51f206')
build() {
  install -D -m755 picasa_upload $startdir/pkg/usr/bin/picasa_upload
}
